<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuLayout">
        <attr name="text" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="CategoryLayout">
        <attr name="icon" />
        <attr name="text" />
        <attr name="offset" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="ExpansionLayout">
        <attr name="text" />
    </declare-styleable>
    <declare-styleable name="PlacesLayout">
        <attr name="text" />
    </declare-styleable>
    <declare-styleable name="ImageBox">
        <attr name="icon" />
        <attr name="background" format="reference" />
    </declare-styleable>
    <!-- Shapes -->
    <declare-styleable name="ShapeFrameLayout">
        <attr name="shape" format="string|reference" />
        <attr name="solidColor" format="color|reference" />
        <attr name="cornerRadius" format="dimension|reference" />
        <attr name="cornerTopLeft" format="dimension|reference" />
        <attr name="cornerTopRight" format="dimension|reference" />
        <attr name="cornerBottomLeft" format="dimension|reference" />
        <attr name="cornerBottomRight" format="dimension|reference" />
        <attr name="borderSize" format="dimension|reference" />
        <attr name="borderColor" format="color|reference" />
    </declare-styleable>
    <declare-styleable name="ShapeLinearLayout">
        <attr name="shape" />
        <attr name="solidColor" />
        <attr name="cornerRadius" />
        <attr name="cornerTopLeft" />
        <attr name="cornerTopRight" />
        <attr name="cornerBottomLeft" />
        <attr name="cornerBottomRight" />
        <attr name="borderSize" />
        <attr name="borderColor" />
    </declare-styleable>
    <declare-styleable name="ShapeRelativeLayout">
        <attr name="shape" />
        <attr name="solidColor" />
        <attr name="cornerRadius" />
        <attr name="cornerTopLeft" />
        <attr name="cornerTopRight" />
        <attr name="cornerBottomLeft" />
        <attr name="cornerBottomRight" />
        <attr name="borderSize" />
        <attr name="borderColor" />
    </declare-styleable>
    <declare-styleable name="ShapeCardView">
        <attr name="shape" />
        <attr name="solidColor" />
        <attr name="cornerRadius" />
        <attr name="cornerTopLeft" />
        <attr name="cornerTopRight" />
        <attr name="cornerBottomLeft" />
        <attr name="cornerBottomRight" />
        <attr name="borderSize" />
        <attr name="borderColor" />
    </declare-styleable>
</resources>